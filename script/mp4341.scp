function "init"
{
	// イベントエリア制御
	EventAreaEnable( "evb2_BossStart" , 0 )
	EventAreaEnable( "evb2_M03S290" , 0 )

	// ストッパー解除
	MapAnime("CT_boss", "open")		// ボス閉じ込めあたり

	// イベント発生制御
	if( FLAG[SF_BOSS_BATTLE] )
	{
		//リトライやデバッグボスジャンプからのボス戦
		if( !FLAG[SF_TIMEATK] &&				// タイムアタック中ではない
			FLAG[GF_SPEEDRUN_SKILL_DANA] &&		// やり込みスキル習得：③▼ダーナＶＳマスターコングを勝利した
			!FLAG[GF_SPEEDRUN_SKILL_LAXIA] && FLAG[SF_LAXIA_JOINOK])	// やり込みスキル習得：④▼ラクシャＶＳマスターコングを勝利した	
		{
			SetWork( WK_BGM , BGM_Nothing )
			EventCue("mp4341:Retry_Speedrun_Monkey4_Battle")
		}
		else
		{
			SetWork( WK_BGM , BGM_Nothing )
			EventCue("mp4341:EV_RetryBoss")
		}
	}
	else if( FLAG[SF_EVENT_ON] )
	{
		// 中ボス開始まだなら中ボス開始する為のイベントボックスオンにする
		if(!FLAG[GF_03MP4341_KILL_ANCIENT] ){	// ジャンダルムの頂上に到着する
			SetWork( WK_BGM , BGM_Nothing )
			SetChrPos("b003" -100000.00f , 00.00f , 00.00f )
			EventAreaEnable( "evb2_BossStart" , 1 )
		}

	//一般配置ファンクション呼び出し
		CallFunc("mp4341:NpcPosition")

	//ＮＰＣマーカーセット
		CallFunc("system:SetMarkerNPC")
	}
}

