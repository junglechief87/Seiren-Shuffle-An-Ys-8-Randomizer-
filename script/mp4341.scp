		MenuClose(10,0)
		
		//──────────────────────
		//	⇒『完熟マンゴー』を献上する
		if (FLAG[TF_MENU_SELECT] == 1)
		{
			//	アイテム譲渡メッセージ
			TalkPopup(UNDEF, 0, 5, 0, 0, 0)
			{
