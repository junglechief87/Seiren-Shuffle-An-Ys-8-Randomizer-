		SetFlag(TF_MENU_SELECT,0)
		MenuReset()
		MenuType(MENUTYPE_POPUP)
//		MenuAdd(1,"『完熟マンゴー』を献上する")
		MenuAdd(1,"#2C#245I完熟マンゴー#0Cを献上する")
