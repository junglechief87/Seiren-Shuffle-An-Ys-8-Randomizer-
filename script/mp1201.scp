			// ショップ処理
			SetFlag(GF_SHOP_VOICE_ALISON, 0)					// アリスンのショップボイスを鳴らすを解除
			ShopOpen( SHOP_ID_DRESS, -1 )		// アリスンの仕立て屋
		}
	}

	ResetStopFlag(STOPFLAG_TALK)
}

