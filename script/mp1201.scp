	case 2:		MapAnime( "Shipyard" , "Lv2" )	break;	// Lv2			20-21[E]	小船建造完了状態です。					イベントで使用？
	case 3:		MapAnime( "Shipyard" , "Lv3" )	break;	// Lv3			30-120[E]	小船建造完了で停泊した状態です。
