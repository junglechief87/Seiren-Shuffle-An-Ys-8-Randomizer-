	}
	// ※Lv0　状態のときは　wishtreeとwishtree_alpha　を子に影響でHide設定にして下さい。
	if( FLAG[GF_CAMP_WISHTREE_LV] == 0 )
	{
		MapHide( "wishtree_alpha" , 0 )
	}
	// ※Lv1　以降のときは　wishtree_alpha　を子に影響で表示設定にして下さい。
	else
	{
		MapHide( "wishtree_alpha" , 1 )
	}

// パロの止まり木LV（個別ノード）
