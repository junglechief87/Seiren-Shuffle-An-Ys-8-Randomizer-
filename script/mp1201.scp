	switch( FLAG[GF_CAMP_PAROWOOD_LV] )	// 漂流村の発展設定：パロの止まり木LV
	{
	case 0:		// Lv0			0-1[E]		(初期状態)未開拓です。
		MapAnime( "parowood" , "Lv0" )
		MapHide( "parowood", 0 )
		break;
	case 1:		// Lv1			10-19[E]	パロの止まり木が各地に出現します。
		MapAnime( "parowood" , "Lv1" )
		MapHide( "parowood", 1 )
		break;
	}

}

