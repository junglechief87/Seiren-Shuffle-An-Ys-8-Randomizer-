	switch( FLAG[GF_CAMP_SHIPYARD_LV] )	// 漂流村の発展設定：造船所LV
	{
	case 0:		MapAnime( "Shipyard" , "Lv0" )	break;	// Lv0			0-1[E]		(初期状態)未開拓です。
