		SetChrWork("ADOL", CWK_FORCELIPSYNC, 0)
		Wait(10)

		HeadYes("Dogi", 1, 25)
		ChangeSubAnimation("Dogi", SUBMOT_EYE, ANI_E_CLOSE_S, 1)
		Wait(15)
		ChangeSubAnimation("Dogi", SUBMOT_EYE, ANI_E_CLOSE_E, 1)
		TalkPopup("Dogi",0,0,0,0,0)
		{
			"#080mおうよ、重たい資材も"
