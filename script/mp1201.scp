	}

	if( FLAG[GF_NPC_7_01_GOTO_TEM] &&								// ▼エピローグ①寺院前へ向かう
		!FLAG[GF_NPC_7_03_CHECK_VILLAGE] )							// ▼エピローグ③漂流村を最後に見回る
	{
		MapAnime( "EVENT_ED" , "on" )	//脱出時の積荷（エピローグ）
