function "Sinai_Aaron"
{

//開始処理ここから----------------------------------------------------

	//CallFunc("system:event_begin")
	SetStopFlag(STOPFLAG_EVENTIMPOSE)
	FadeOut(FADE_BLACK,0)
	WaitFade()

	LoadChr("Aaron")

	SaveEventState()
