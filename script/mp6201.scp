			// タナトス			//	場所		：mp6201
			SetChrPos("Thanatos",-100000.0f, 0.0f, 0.0f)
			Turn("Thanatos",-26.15f,360.0f)
			ChangeAnimation( "Thanatos", "ANI_EV_RYOTEKOSI", -1, 1)	//両手を腰にやる
			//ChangeChrPos("Thanatos",		"currentmap", "D_Thanatos"		)		// 「現在のマップ」にいなければ、マーカー用ダミーキャラと座標を入れ替える
		}
		else
		{
			// タナトス			//	場所		：mp6204
		}
	}
}


