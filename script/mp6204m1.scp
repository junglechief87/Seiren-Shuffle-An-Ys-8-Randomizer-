function "EV_Quake"
{
		while(1)
		{
			Wait((20 + rand() % 180))	// 適当にばらけさせる
			EarthQuake( 0.02f , 0.015f , 60 )	// 縦揺れ幅, 横揺れ幅, 時間		//◆仮処理
			Wait(60)
		}
}

