//──────────────────────────────
//	【男性】※メインイベントに登場したキャラ
//──────────────────────────────
function "Talk_Citizen_5_B01"
{
	SetStopFlag(STOPFLAG_TALK)
	CallFunc("system:talk_begin_zoom")		//TALK_ZOOM

	LookChr("this","LEADER")

	if( FLAG[GF_PAST_EPISODE_5_GO_VALLAY] )			//▼過去編Ⅴ王家の谷へ向かう
	{
		//▼過去編Ⅴ王家の谷へ向かう
		//───────────────
		//	場所		：西街道①（過去）
		//───────────────
		//	配置・カメラ：道の脇／そばに竜車を止める
		//	モーション	：デフォルトで悲哀表情
		//───────────────
		//	男性
		TalkPopup("this",0,0,0,0,0)
		{
			"穀倉地帯まで行けば"
			"食うものには困らんはずだ……\p"
			"だってそうだろう！？"
			"去年はあんなに豊作だったじゃないか！！"
		}
		WaitPrompt()
		WaitCloseWindow()
	}

	ResetStopFlag(STOPFLAG_TALK)
	Look("Citizen_5_B01" , 0 , -20 )
}


