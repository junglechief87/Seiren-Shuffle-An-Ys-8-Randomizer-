function "Talk_Lakey"
{
	SetStopFlag(STOPFLAG_TALK)
	CallFunc("system:talk_begin_zoom")		//TALK_ZOOM

	if( FLAG[GF_PAST_EPISODE_5_GO_VALLAY] )			//▼過去編Ⅴ王家の谷へ向かう
	{
		
		//▼過去編Ⅴ王家の谷へ向かう
		//───────────────
		//	場所		：西街道②（過去）
		//───────────────
		//	配置・カメラ：道の脇／そばに竜車を止める
