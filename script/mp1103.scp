// initスクリプト
function "init"
{
	
	//遠景不必要ノード削除◆マップ作業者用
	MapHide( "@BG2_mp1111" , 0 )
	MapHide( "@BG2_mp1112_3" , 0 )
	MapHide( "@BG2_mp1103" , 0 )
	MapHide( "@BG2_mp1131" , 0 )
	MapHide( "@BG2_mp3101_3" , 0 )
	
	// ストッパー
	MapAnime("CT_EVENT","open")

	//イベントエリア制御
	EventAreaEnable( "evb2_M01S180" , 0 )
	EventAreaEnable( "ST_1103_to1104" , 0 )	// 【ストッパー：1103⇒1104】

	//幽霊船濃霧
	MapHide( "skyfog2" , 0 )		//濃霧非表示
	if( FLAG[SF_EVENT_ON] ){
		if( FLAG[GF_05MP6330_LOOK_MONOLITH] && !FLAG[GF_NPC_5_03_GOTO_GHOSTHIP] ){
			MapHide( "skyfog2" , 1 )		//濃霧表示する
			MapHide( "BG2_sky1" , 0 )		//元の空非表示
			MapHide( "BG2_sky2" , 0 )		//元の空非表示

			SetFog(FOG_BOTH, 0x00828c9e, 0.0f, 120.0f , 2.0f, 0.3f, 0.9f, FOGPARAM_MAP_DEFAULT, FOGPARAM_MAP_DEFAULT, 0)
			SetFogBack(FOG_BOTH, 0x00939aa0, 200.0f, 1000.0f , 1.0f , 0.7f , 0.9f , FOGPARAM_MAP_DEFAULT, FOGPARAM_MAP_DEFAULT, 0 )
			SetMapLightColor( 1.0f, 1.0f, 1.0f, 1.0f, 0.8f )
		}

		// ストッパー制御
		if( FLAG[GF_02MP1111_WOUND_BARBAROSS] &&	// バルバロスが致命傷を負う
			!FLAG[GF_02MP1103_KILL_KIERGAARD] )			//ダーナ編①	植樹祭の説明を受ける
		{
			EventAreaEnable( "ST_1103_to1104" , 1 )	// 【ストッパー：1103⇒1104】
		}
	}

	// イベント発生制御
	if( FLAG[SF_BOSS_BATTLE] )
	{
		//リトライ・デバッグボスジャンプからのボス戦
		if (FLAG[GF_MP1103_KIERGAARD])
		{
			SetWork( WK_BGM , BGM_Nothing )
			EventCue("mp1103:EV_RetryBoss2")	// キルゴール戦
		}
		else
		{
			SetWork( WK_BGM , BGM_Nothing )
			EventCue("mp1103:EV_RetryBoss")		// 古代獣戦
		}
	}
	else if( FLAG[SF_EVENT_ON] )
	{
		if( FLAG[GF_TBOX_DUMMY060] && !FLAG[GF_01MP1103_JOIN_SAHAD] && !FLAG[GF_EVENT_CTL] )
		{
			SetWork( WK_BGM , BGM_Nothing )
			EventAreaEnable( "evb2_M01S180" , 1 )
		}
		if( FLAG[GF_02MP1111_WOUND_BARBAROSS] && !FLAG[GF_02MP1103_KILL_KIERGAARD] && !FLAG[GF_EVENT_CTL] )
		{
			EventCue("mp1103:EV_M02S660")
		}
	}

	//サウンド制御
	if( FLAG[GF_02MP1111_WOUND_BARBAROSS] && !FLAG[GF_02MP1103_KILL_KIERGAARD] ){	//
		SetWork( WK_BGM , BGM_EVENT_011 )
	}

}

// 古代獣戦
function "EV_RetryBoss"
{
	CallFunc("rng:bossScaling")

	SetStopFlag(STOPFLAG_EVENT)
	FadeOut(FADE_BLACK,0)
	WaitFade()

	//ボス配置・アニメ--------------------------------
	SetChrPos(B150,47.01f,-1222.10f,8.06f)
	Turn(B150,-75.00f,360.0f)

	//PLAYER配置--------------------------------------
	SetChrPos(PLAYER1,62.73f	-1225.76f	8.04f)
	Turn(PLAYER1,109.52f,360.0f)

	//カメラ設定--------------------------------------
	// argの設定を上書きする
	SetYs8Camera(8.0f, 60.0f, 14.0f, 0.3f, 0.3f)

	MoveCameraAt( 0 , 62.730f , -1225.760f , 10.183f , 0 )	// 注視点
	ChangeCameraDistance( 0 , 8.000f , 0 )		// 基本距離
	ChangeCameraElevation( 0 , 7.062f , 0 )	// 基本仰角
	RotateCamera( 0 , 75.830f , 0 )				// 角度
	ChangeCameraPers( 0 , 60.000f , 0 )			// 視野角
	RollCamera( 0 , 0.0f , 0 )				// ロール回転

	//イベントバトル用CallFunc------------------------
	CallFunc("system:boss_start")

	//その他処理--------------------------------------
	PlayBGM(BGM_BOSS_003, 0)
	MapAnime("CT_EVENT","close")	//逃げられないストッパー起動

	if( FLAG[TF_NO_RETRY] )
	{
		SetFlag( TF_NO_RETRY, 0 )

		//スキップでとんできた場合
if(0)//		if (!FLAG[SF_TOPIC_17])	フラグ名変わるのでコメントアウト
		{
			//自動送り禁止
			ResetStopFlag(STOPFLAG_AUTOMESSAGE)

			//■▼チュートリアル：ターゲットロック（mp7304）
			Wait(10)

			PlaySE( SE_TUTORIAL , 100 , 0,  0, 0, 1.0f, 0)		//チュートリアル
			Portrait_Anime(  1 , PR_ANIME_COLOR, 1.0f, 1.0f, 1.0f, 1.0f,FADE_NORMAL)
			Wait(FADE_NORMAL)
			Message2(1,TOPIC_BUTTUN_X,TOPIC_BUTTUN_Y,"","","","","","","","","","")		//ボタン入力可能を提示
			WaitPrompt()
			Portrait_Anime(  1 , PR_ANIME_COLOR, 1.0f, 1.0f, 1.0f, 0.0f,FADE_FAST)
			Wait(FADE_FAST)
			Portrait_Close(  1 )
			Portrait_Unload( 1 )

//			SetFlag( SF_TOPIC_17, 1 )		//ターゲット

			//自動送り許可
			SetStopFlag(STOPFLAG_AUTOMESSAGE)
		}
		ResetStopFlag(STOPFLAG_EVENT)
	}
	else
	{
		ResetStopFlag(STOPFLAG_EVENT)
	}

	//------------------------------------------------
	if (FLAG[SF_TIMEATK])
	{
		// タイムアタックの場合はボス名出してあげる
		VisualName("visual/bossname/bn_b150.itp", VN_NAMEBOSS2, VN_BOSSNAME_LT_X, VN_BOSSNAME_LT_Y,VN_BOSSNAME_TIME)
	}

	FadeIn(FADE_BLACK,FADE_FAST)
	WaitFade()
}

