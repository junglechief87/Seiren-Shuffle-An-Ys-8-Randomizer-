	ChangeCameraDistance( 0 , 13.737f , 0 )		// 基本距離
	ChangeCameraElevation( 0 , 28.803f , 0 )	// 基本仰角
	RotateCamera( 0 , 1.950f , 0 )				// 角度
	ChangeCameraPers( 0 , 60.000f , 0 )			// 視野角
	SetCameraZPlane( 0.100f , 600.000f )			// ZPlane
