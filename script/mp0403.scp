	FadeOut(FADE_BLACK, FADE_FAST)

	Message("The path is opened.")
	
	//終了処理はスキップ禁止
	SetSkipScript("")

	//イベント前のキャラ情報を復帰
	RestoreEventState()

	CallFunc("system:default_Shake_set")	// デフォルト船揺れ

	//隠したものを復帰。
	ResetChrPos((CHRTYPE_MONS | RESET_POS | RESET_AISTATE))

