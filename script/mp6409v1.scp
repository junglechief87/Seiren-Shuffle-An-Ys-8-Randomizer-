function "init"
{

	//ノード・村施設設定
	MapAnime("mistSphere", "waint1")	// 霧の壁
	//	wait1			1-180			ループです(初期状態)
	//	expand			180-600			霧が拡大していくアニメです
	//	wait2			600-780			霧が拡大した状態のループです
	//	open			780-1200		霧が割れるアニメです
	//	wait3			1200-1380		霧が開いた状態のループです

	EventAreaEnable( "evb2_M06S110" , 0 )

	if( FLAG[SF_EVENT_ON] ){

		//ノード制御
		if (FLAG[GF_06MP6202M_GOTO_GARDEN])	{	// ▼[M06S050][過去Ⅵ　セレンの園へ向かう]
			MapAnime("mistSphere", "wait2")	// 霧の壁
		}

		// イベント発生制御
		if (FLAG[GF_06MP4105B_NONE_PARTY]			// ▼[M06S042][霧の中で異形の古代種と戦う２]
			&& !FLAG[GF_06MP6202M_GOTO_GARDEN]) {		// ▽[M06S050][過去Ⅵ　セレンの園へ向かう]
			EventCue("mp6409v1:EV_M06S043")
		}
		if (FLAG[GF_06MP6350_TALK_GUARDIAN]			// ▼[M06S090][アドルたちがセレンの園に到着する	]
			&& !FLAG[GF_06MP4109_CLEAR_MIST]) {		// ▽[M06S111][山門の霧を突破する２				]
			EventAreaEnable( "evb2_M06S110" , 1 )
		}

		//本物の水晶石を一時的にどかす
		if(!FLAG[GF_06MP6202M_GOTO_GARDEN]){		// ▽[M06S050][過去Ⅵ　セレンの園へ向かう]
			SetChrPos("chkpt",-100000.0f,0.00f,0.00f)
		}

		if( FLAG[GF_06MP4109_GOTO_GARDEN] && //アドルがセレンの園を目指す
			!FLAG[GF_06MP4109_CLEAR_MIST] )	//山門の霧を突破する
		{
			// ＬＰ制御
			//SetChrPos("chkpt_dummy", -100000.0f, 0.0f, 0.0f)	// イベント用水晶石を撤去
			//SetChrPos("LP_SubEV_Past_06_Rearr_A", -271.87f, 584.58f, 110.17f)	// 過去編Ⅵ：【過去編Ⅵ：水晶石を調べて再訪する】
			//Turn2("LP_SubEV_Past_06_Rearr_A",159.59f, 360.0f)
			//SetChrWork( "LP_SubEV_Past_06_Rearr_A" , CWK_CHECKRADIUS , 2.5f )		// 範囲
		}
	}

	//サウンド制御
//	if( FLAG[GF_06MP4109_GOTO_GARDEN] ){	//
//		SetWork( WK_BGM , BGM_EVENT_008 )
//	}

}


