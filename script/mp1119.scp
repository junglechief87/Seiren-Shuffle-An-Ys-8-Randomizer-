function "NpcPosition"
{
	if( FLAG[GF_QUEST_400] >= QUEST_SUCCESS &&		// ししょーへの献上（リコッタ）
		!FLAG[GF_SPEEDRUN_SKILL_SAHAD] && FLAG[SF_SAHAD_JOINOK])			// やり込みスキル習得：②▼サハドＶＳマスターコングを勝利した
	{
		//マスターコング
		SetChrPos("Monkey",785.02f, 785.21f, 3.41f)
		Turn("Monkey",-78.27f,360.0f)
		ChangeAnimation( "Monkey", "ANI_EV_UDEGUMI", -1, 1 )
	}
}

