//──────────────────────────────

function "EV_M07S010"			// ＥＤⅠ 寺院跡を訪れる
{
	//開始処理ここから-----------------------------------------------
	SetStopFlag(STOPFLAG_EVENT)
	FadeOut(FADE_BLACK, 0)
	WaitFade()

	//イベント開始前情報の保存
	SaveEventState()
	SaveCamera()

	//パーティ状態のリセット
	CallFunc("system:party_reset")
	CallFunc("system:party_remove")

	//環境の初期化・不要なものを隠す
	ClearDebris()
	ResetChrPos((CHRTYPE_MONS | REMOVE_POS))
	//ResetChrPos((CHRTYPE_NPC | REMOVE_POS))

	//キャラ初期状態の設定				
	SetChrWork("ADOL", CWK_DEFAULT_SKIPNODE_OFF, 1)
	SetChrPos("ADOL", 14.35f, 146.68f, 80.34f)
	Turn("ADOL", -12.12f, 360.0f)
	ChangeAnimation("ADOL", "ANI_WAIT", -1, 1)
	ChangeSubAnimation("ADOL", SUBMOT_EYE, ANI_E_WAIT, 1)
	ChangeSubAnimation("ADOL", SUBMOT_MOUTH, ANI_M_WAIT, 1)
	ChangeSubAnimation("ADOL", SUBMOT_EXT1, ANI_E_LOOKC, 1)
	LookReset("ADOL")
	SetChrInfoFlag("ADOL", INFOFLAG_NOCHRATARI)

	//BGM設定◆
	//PlayBGM( BGM_FIELD_101 , XX )

	// 初期カメラ
	MoveCameraAt( 0 , 14.133f , 159.826f , 85.445f , 0 )	// 注視点
	ChangeCameraDistance( 0 , 6.600f , 0 )		// 基本距離
	ChangeCameraElevation( 0 , -19.651f , 0 )	// 基本仰角
	RotateCamera( 0 , 148.998f , 0 )				// 角度
	ChangeCameraPers( 0 , 80.000f , 0 )			// 視野角
	SetCameraZPlane( 0.500f , 900.000f )			// ZPlane
	RollCamera( 0 , 0.0f , 0 )				// ロール回転
	// ResetCameraZPlane()	// イベント終了時に呼び出して下さい。マップ側の設定に戻します。

	// 被写界深度の設定
	SetDoF(0, LERP_LINEAR, 5.000, 350.000 ,0.000 ,0.700 ,0.500)

	// グレアの設定
	SetGlare(0, LERP_LINEAR, 0.700, 1.300, 1.000, 1.000, 0.600, 0.000, 1.000)	//イベント毎に調整

	//スキップ許可
	SetSkipScript("mp6409v2:EV_M07S010_ED")

	//開始処理ここまで-----------------------------------------------

	//　　寺院の山門があった場所までやって来るが、
	//　　山門どころか寺院そのものが見当たらない。
	//　　古代種もおらず植生も南部と同じものになっている。
	//　　寺院マップへ入ったところでイベント開始。
	//　　そこにはただ樹木と草が生い茂り、
	//　　静かに木漏れ日が差し込み、風がそよいでいる状況。
	//　　スクロールさせつつ寺院跡地を見せて
	//　　画面フェードアウト。
	
	// カット1 ------------------------------------------------------------------------------------------
	
	// カメラ移動
	ExecuteCmd(1000, MoveCameraAt, INTERPOLATE_SPHERE ,16.942f , 154.171f , 85.445f, 300 )

	// 非表示・再表示
	SetChrInfoFlag("ADOL", INFOFLAG_INVISIBLE)	// 非表示

	PlayBGM(BGM_TOWN_201, 0)

	// フェードイン
	FadeIn(FADE_BLACK, FADE_SLOW)
	WaitFade()
	
	Wait(60)
	
	// テキスト（色替え）
	TalkPopup(UNDEF, 0, 3, SYSTEM_PPOSX, SYSTEM_PPOSY, 0){
		"#7Cそこには何も残っていなかった。"
		"#7C寺院も、そして《はじまりの大樹》も……"
	}
	WaitPrompt()
	WaitCloseWindow()
	
	Wait(40)

	// カット2 ----------------------------------------------------------------------------------------

	// クロスフェード
	CrossFade(45)
	
	// カメラ停止
	StopThread(1000)
	
	// カメラ
	MoveCameraAt( 0 , 14.310f , 146.451f , 81.781f , 0 )	// 注視点
	ChangeCameraDistance( 0 , 2.214f , 0 )		// 基本距離
	ChangeCameraElevation( 0 , -8.420f , 0 )	// 基本仰角
	RotateCamera( 0 , 303.668f , 0 )				// 角度
	ChangeCameraPers( 0 , 60.000f , 0 )			// 視野角
	SetCameraZPlane( 0.500f , 900.000f )			// ZPlane
	RollCamera( 0 , 0.0f , 0 )				// ロール回転
	// ResetCameraZPlane()	// イベント終了時に呼び出して下さい。マップ側の設定に戻します。

	// 非表示・再表示
	ResetChrInfoFlag("ADOL", INFOFLAG_INVISIBLE)	// 再表示

	// カメラ移動
	ExecuteCmd(1000, MoveCameraAt, INTERPOLATE_SPHERE , 14.168f , 141.133f , 81.916f , 270 )
	ExecuteCmd(1001, ChangeCameraDistance, INTERPOLATE_SPHERE , 0.997f , 270 )
	ExecuteCmd(1002, ChangeCameraElevation, INTERPOLATE_SPHERE , -8.122f , 270 )
	ExecuteCmd(1003, RotateCamera, INTERPOLATE_SPHERE , 31.747f , 270 )

	SetMotSpd("ADOL" ,-1 , 0.7f )	// モーションスピード変更
	LookSpd("ADOL", 0.08f, 0.08f, 0.08f )// ゆっくりめに首向き

	// 移動
	ExecuteCmd(1, MoveTo, "ADOL", 14.18f, 141.17f, 80.41f, 0.1f, 0.09f)
	
	// フェード待ち
	Wait(45)

	Wait(20)
	LookSpd("ADOL",0.08f,0.08f,0.08f)
	Look("ADOL", 30.0f, 5.0f)	// ←↑（＾＾）
	Wait(80)
	Look("ADOL", -35.0f, 0.0f)	// （＾＾）→
	Wait(80)
	LookReset("ADOL")

	// テキスト（色替え）
	TalkPopup(UNDEF, 0, 3, SYSTEM_PPOSX, SYSTEM_PPOSY, 0){
		"#7Cここには初めから何もありはしない……"
		"#7Cそう語りかけるかのように"
		"#7C苔むした樹木と草が鬱蒼と生い茂っている。"
	}
	WaitPrompt()
	WaitCloseWindow()

	// 移動待ち
	WaitThread(1)
	
	ChangeSubAnimation("ADOL", SUBMOT_EYE, ANI_E_CLOSE_S, 1)	// 目
	Look("ADOL", 0.0f, -10.0f)	// うつむく
	Wait(20)

	// テキスト（色替え）
	TalkPopup(UNDEF, 0, 3, SYSTEM_PPOSX, SYSTEM_PPOSY, 0){
		"#7Cただ、木漏れ日だけが"
		"#7Cアドルを優しく包んでいた。"
	}
	WaitPrompt()
	WaitCloseWindow()

	// カメラ待ち
	WaitThread(1000)
	WaitThread(1001)
	WaitThread(1002)
	WaitThread(1003)

	LookSpd( "ADOL", LOOKSPD_DEFAULT, LOOKSPD_DEFAULT, LOOKSPD_DEFAULT )

	// カット3 ----------------------------------------------------------------------------------------

	// クロスフェード
	CrossFade(45)

	// カメラ
	MoveCameraAt( 0 , 19.380f , 96.385f , 98.040f , 0 )	// 注視点
	ChangeCameraDistance( 0 , 6.897f , 0 )		// 基本距離
	ChangeCameraElevation( 0 , -24.014f , 0 )	// 基本仰角
	RotateCamera( 0 , 145.277f , 0 )				// 角度
	ChangeCameraPers( 0 , 80.000f , 0 )			// 視野角
	SetCameraZPlane( 0.500f , 900.000f )			// ZPlane
	RollCamera( 0 , 0.0f , 0 )				// ロール回転
	// ResetCameraZPlane()	// イベント終了時に呼び出して下さい。マップ側の設定に戻します。


	// 被写界深度の設定
	SetDoF(0, LERP_LINEAR, 5.000, 450.000 ,0.000 ,0.700 ,0.500)

	// カメラ移動
	ExecuteCmd(1000, MoveCameraAt, INTERPOLATE_SPHERE , 15.685f , 122.925f , 86.381f, 900 )
	ExecuteCmd(1001, ChangeCameraDistance, INTERPOLATE_SPHERE , 6.897f , 900 )
	ExecuteCmd(1002, ChangeCameraElevation, INTERPOLATE_SPHERE , -22.055f , 900 )
	ExecuteCmd(1003, RotateCamera, INTERPOLATE_SPHERE , 145.277f , 900 )

	// フェード待ち
	Wait(45)

	Wait(60)

	// テキスト（色替え）
	TalkPopup(UNDEF, 0, 3, SYSTEM_PPOSX, SYSTEM_PPOSY, 0){
		"#7Cその後、アドルは島の各所を巡ったが……\p"
		"#7Cエタニアの遺構は残っているものの、"
		"#7C大樹やその巫女が存在した痕跡を"
		"#7C見つけることはついにできなかった。"
	}
	WaitPrompt()
	WaitCloseWindow()

	Wait(60)

	StopBGM(120)

	// フェードアウト
	FadeOut(FADE_BLACK, FADE_SLOW)
	Wait(30)
	SetEnvSEPlayState(-1, 0)	//環境SE全停止
	WaitFade()
	
	// カメラ停止
	StopThread(1000)
	StopThread(1001)
	StopThread(1002)
	StopThread(1003)

	Wait(30)
	
	// カット4（暗転） ---------------------------------------------------------------------------------
	
	// テキスト（色替え）
	TalkPopup(UNDEF, 0, 3, SYSTEM_PPOSX, SYSTEM_PPOSY, 0){
		"#7Cそして、瞬く間に一週間が過ぎ……\p"
		"瞬く間2行目"
		"#7Cついに漂流者たちがセイレン島を"
		"#7C脱出する日を迎えたのだった。"
	}
	WaitPrompt()
	WaitCloseWindow()
	
	//終了処理ここから----------------------------------------------------
	CallFunc("mp6409v2:EV_M07S010_ED")
}

function "EV_M07S010_ED"
{
	//終了処理はスキップ禁止
	SetSkipScript("")

	//キャラモーション初期化
	CallFunc("system:reset_chrmot_ALL")

	//マップパラメーターリセット
	ResetMapParam(-1)


	//NowLoading 時の Tips 表示をカット
	SetFlag(TF_LOADING_TIPS_OFF, 1)

	//イベント・ギャラリー中
	if(FLAG[TF_EVSKIP] && FLAG[SF_GALLERY_EVENT])
	{
		FadeBGM(100,15)
		SoundEfx( SEFX_DEFAULT, 15 )
		SetEnvSEPlayState(-1, 0)	//環境SE全停止
		GalleryEvEnd(0)		// イベントギャラリー終了（スクリプトから強制的に抜けていろいろ処理してくれる）
		ResetStopFlag(STOPFLAG_EVENTIMPOSE)
	}
	// 通常時
	else
	{
		//マーカー
		DelMapMarker( SMI_SYMBOL, PAGE_MP120x, MARKER_EV_M07S001, 0,0)	//出口

		StopBGM(30)
		SetFlag(SF_NOCHANGEBGM, 1)
		
		LoadArg("map/mp1201t3/mp1201t3.arg")
		EventCue("mp1201:EV_M07S020")	// NEXT：ＥＤⅠ セイレン島から脱出する

		SetEnvSEPlayState(-1, 0)	//環境SE全停止

		ResetStopFlag(STOPFLAG_EVENT)
	}
	//終了処理ここまで----------------------------------------------------
}

