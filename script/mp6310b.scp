		StopEmotion( "ALL" )
		ResetMoveVec("PARTYALL")

		SetChrPos("LEADER", 0.0f, -40.0f, 0.00f)
		Turn("LEADER", 180.0f, 360.0f)
		ResetPartyPos()
		ResetFollowPoint()

