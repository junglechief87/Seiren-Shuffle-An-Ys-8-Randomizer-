//-------------------------------------------
// init
function "init"
{
	
	if( FLAG[SF_EVENT_ON] )
	{
		//ヒモロギの成長度
		CallFunc("system:himorogi_grow")

		if( FLAG[GF_TBOX_DUMMY087]				// ▼[M06S020][調査のためジャンダルムへ向かう]
			&& !FLAG[GF_06MP4109_CLEAR_MIST] )		// ▽[M06S111][山門の霧を突破する２]
		{
			MapAnime("mistSphere", "on")	// 霧のマユ
		}
	}
}

