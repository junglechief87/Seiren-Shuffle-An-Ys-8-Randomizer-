//	ExecuteCmd(1001, RotateCamera, INTERPOLATE_SPHERE, 30.000f, 160)		// 角度
//	ExecuteCmd(1002, ChangeCameraZoom, INTERPOLATE_SPHERE, 1.100f, 160)		// ズーム値
//	
//	// モンスター攻撃モーション
////	ChangeAnimation("ev_mons00","ANI_ATK1",-1,1)
