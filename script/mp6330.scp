// initスクリプト
function "init"
{
	
	EntryOn(0, 0)		// mp6329 リフトのエントリーoff

	if( FLAG[SF_EVENT_ON] )
	{
		if( !FLAG[GF_TBOX_DUMMY128] ){
			SetChrPos("LP_Monolith",25.82f,0.37f,593.88f)
		}
//		if (FLAG[GF_05MP6329_KILL_BAHABOSS] && !FLAG[GF_05MP6330_FIND_MONOLITH]){
//			EventCue("mp6330:EV_M05S120", 1)
//		}

		// ＬＰ制御
		if( !FLAG[GF_TBOX_DUMMY128] )
		{
			SetChrPos("LP_6330_Monolith_C", -100000.00f, 0.00f, 0.00f)	// 【ＬＰ：知恵の間の中央モノリス・その後】
		}

		// クエスト制御
		// 【QS610】親友の遺言（ダーナ）	▼バハの塔の石碑を調べる
		if( FLAG[GF_QUEST_610] == QUEST_START &&	// 【QS610】親友の遺言（ダーナ）	クエストを開始した
			FLAG[GF_QS610_LOOK_DESK] &&				// 【QS610】▼中央書架の机を調べる を見た
			!FLAG[GF_QS610_LOOK_STELE] )			// 【QS610】▼バハの塔の石碑を調べる を見た
		{
			SetChrPos("LP_QS_610_Look_Stele", 26.25f,0.52f,594.21f)
			SetChrWork( "LP_QS_610_Look_Stele" , CWK_CHECKRADIUS , 2.5f )		// 範囲
		}
	}
}

function "mono_boot"
{

	//認証画面起動
	MapAnime("locator_mono" , "white_open" )
	PlaySE(SE_M05S130_02, 100, 0, 0, 0, 1.0f, 0)

	//認証画面閉じる
	MapAnimeLast("locator_mono" , "white_close" )

	//通常画面起動
	MapAnimeLast("locator_mono" , "open1" )

	//ループ
	MapAnimeLast("locator_mono" , "open1_wait" )

	Wait(30)
	PlaySE(SE_M05S130_04, 100, 0, 0, 0, 1.0f, 0)
	Wait(30)
	PlaySE(SE_M05S130_02, 60, 0, 0, 0, 1.0f, 0)
	Wait(5)
	PlaySE( SE_M05S130_03 , 100 , 0,  0, 0, 1.0f, 0)	//◆認証OK

	//モノリス発光
	MapAnime("glow00" , "on" )

}


function "mono_serch"
{
	//通常窓閉じる
	MapAnime("locator_mono"       , "open1_close" )
	PlaySE(SE_M05S130_04, 100, 0, 0, 0, 1.0f, 0)

	Wait(15)

	//周辺窓開く
	MapAnime("locator_mono_outer" , "open1" 	)

	//検索画面開始
	MapAnimeLast("locator_mono"       , "open4" )

	//ループ
	MapAnimeLast("locator_mono"       , "open4_wait")
	MapAnimeLast("locator_mono_outer"       , "wait1")
	PlaySE(SE_M05S130_05, 100, 0, 0, 0, 1.0f, 0)
	
}


function "mono_oceanus"
{
	//窓一旦閉じる
	MapAnime("locator_mono" , "open4_close" 	)
	MapAnime("locator_mono_outer" , "close1" 	)
	PlaySE(SE_M05S130_04, 100, 0, 0, 0, 1.0f, 0)


	//通常窓（オケアノス画面）開く
	MapAnimeLast("locator_mono" , "open5" )
	MapAnimeLast("locator_mono" , "open5_wait" )
	PlaySE(SE_M05S130_05, 100, 0, 0, 0, 1.0f, 0)

	//周辺窓再展開
	MapAnimeLast("locator_mono_outer" , "open2" )
	MapAnimeLast("locator_mono_outer" , "wait2" )
	Wait(30)
	PlaySE(SE_M05S130_03, 80, 0, 0, 0, 1.0f, 0)
	Wait(60)
	PlaySE(SE_M05S130_05, 100, 0, 0, 0, 1.0f, 0)

}

function "mono_wildly"
{
	//オケアノス画面いったん閉じ
	MapAnime("locator_mono" , "open5_close" )
	PlaySE(SE_M05S130_04, 100, 0, 0, 0, 1.0f, 0)

	//暴走スタート
	MapAnimeLast("locator_mono" , "open6_wait" 	)

	//周辺窓も暴走
	MapAnime("locator_mono_outer" , "close2" 	)
	MapAnimeLast("locator_mono_outer" , "open1" 	)
	MapAnimeLast("locator_mono_outer" , "wait1" 	)
	Wait(30)
	PlaySE(SE_M05S130_06, 100, 0, 0, 0, 1.0f, 0)
	PlaySE(SE_M05S130_05, 100, 0, 0, 0, 1.0f, 0)
}


function "mono_exit"
{
	MapAnime("locator_mono"	,"open2_close" )
	MapAnime("locator_mono_outer","close1")
	PlaySE(SE_M05S130_04, 100, 0, 0, 0, 1.0f, 0)

	Wait(30)
	PlaySE(SE_M05S130_04, 100, 0, 0, 0, 1.0f, 0)
	Wait(10)

	MapAnime("glow00" , "off")
}


//-------------------------------------------
// リフト到着 from mp6329
function "lift_00_end"
{
	SetStopFlag(STOPFLAG_PC)
	SetStopFlag(STOPFLAG_EVENT)

	FadeOut(FADE_BLACK, 0)
	WaitFade()

	//イベント後の再配置位置
	SetChrPos("PLAYER1",-24.29f, 0.73f, 592.43f)
	Turn("PLAYER1" ,-87.32f,360.0f)
	ResetPartyPos()
	ResetFollowPoint()

	//カメラ位置復帰or初期位置設定
	MoveCameraAt( 0 , -24.291f , 0.726f , 594.33f , 0 )	// 注視点
	ChangeCameraDistance( 0 , 10.000f , 0 )		// 基本距離
	ChangeCameraElevation( 0 , 23.255f , 0 )	// 基本仰角
	RotateCamera( 0 , 87.320f , 0 )				// 角度
	ChangeCameraPers( 0 , 60.000f , 0 )			// 視野角
	SetCameraZPlane( 0.100f , 200.000f )			// ZPlane
	RollCamera( 0 , 0.0f , 0 )				// ロール回転
	// ResetCameraZPlane()	// イベント終了時に呼び出して下さい。マップ側の設定に戻します。

	CallFunc("system:camera_reset")

	SetFlag(SF_ALLMUTEKI,0)		//無敵ＯＦＦ
	ResetStopFlag(STOPFLAG_PC)
	ResetStopFlag(STOPFLAG_EVENT)

	FadeIn(FADE_BLACK,FADE_FAST)
	//WaitFade()
}

//-------------------------------------------
// リフト起動 to mp6329
function "lift_00"
{
	SetFlag(SF_ALLMUTEKI,1)		//無敵ＯＮ
	SetStopFlag(STOPFLAG_PC)
	SetStopFlag(STOPFLAG_EVENT)
	SaveCamera()
	SetChrWork("lookp00", CWK_CHECKOFF, 1)		// 普通のルックポイントをオフに

//	PlaySE("SE_GIM_PLANT_FALL1", 100, 0,  0, 0, 1.0f, 0)	// 動く音
	FadeOut(FADE_BLACK,FADE_FAST)
	WaitFade()

	MoveCameraAt( 0 , -24.291f , 0.726f , 594.33f , 0 )	// 注視点
	ChangeCameraDistance( 0 , 10.000f , 0 )		// 基本距離
	ChangeCameraElevation( 0 , 23.255f , 0 )	// 基本仰角
	RotateCamera( 0 , 87.320f , 0 )				// 角度
	ChangeCameraPers( 0 , 60.000f , 0 )			// 視野角
	SetCameraZPlane( 0.100f , 200.000f )			// ZPlane
	RollCamera( 0 , 0.0f , 0 )				// ロール回転
	// ResetCameraZPlane()	// イベント終了時に呼び出して下さい。マップ側の設定に戻します。

	SetChrPos("PLAYER1",-24.29f, 0.73f, 592.43f)
	Turn("PLAYER1" ,-87.32f,360.0f)
	ResetPartyPos()
	ResetFollowPoint()

	Wait(10)

	FadeIn(FADE_BLACK,FADE_FAST)
	WaitFade()

	ExecuteCmd(1002, MoveCameraAt, INTERPOLATE_SPHERE , -24.291f , 0.726f , 591.33f , 200 )
	Wait(40)

	PlaySE("SE_GIM_LIFT_SCFI", 100, 1,  0, 0, 1.0f, 0)	// gimse 動く音 loop
	PlaySE("SE_GIM_LIFT_STOP", 60, 0,  100, 0, 1.0f, 0)	// gimse 動く音
	MapAnime( "lift00" , "down" )
	MapAnime( "CA_10" , "down" )
	Wait(80)
	MapAnime( "lift00" , "down_loop" )
	MapAnime( "CA_10" , "down_loop" )
//	Wait(20)
	StopSE("SE_GIM_LIFT_SCFI", 1000)	// gimse 動く音 stop loop

	FadeOut(FADE_BLACK,FADE_FAST)
	WaitFade()
	RestoreCamera(0,0)

	LoadArg("map/mp6329/mp6329.arg")
	EventCue( "mp6329:lift_01_end" )
}

