		GetTrophy("TROPHY_SCENE_C5_TOWER_END")		//古の斜塔			《バハの斜塔》を攻略した。

		ResetStopFlag(STOPFLAG_EVENTIMPOSE)

		PlayBGM(BGM_6330, 0)

