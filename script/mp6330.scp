		ResetPartyPos()
		ResetFollowPoint()

		//マップマーカー
		DelMapMarker( SMI_SYMBOL ,PAGE_MP632x, MARKER_EV_M05S130, 0,0)		//メイン：ダーナがモノリスを起動する

		SetMapMarker( SMI_SYMBOL ,PAGE_MP120x, MARKER_EV_M05S142, 14.02f, -1425.93f, 17.31f, 14.02f, -1425.93f,MARKER_EV_M05S142,MN_T_VILLAGE_MP1201,0)		//メイン：幽霊船を目撃する

		//カメラ位置復帰or初期位置設定
		MoveCameraAt( 0 , 18.134f , 0.698f , 593.895f , 0 )	// 注視点
		ChangeCameraDistance( 0 , 10.000f , 0 )		// 基本距離
		ChangeCameraElevation( 0 , 12.554f , 0 )	// 基本仰角
