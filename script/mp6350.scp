function "init"
{
	//長すぎるエントリ部分を自動開封
	OpenMinimap( PAGE_MP634x,3,850.0f,0.0f, 25)
	OpenMinimap( PAGE_MP634x,3,834.0f,0.0f, 25)

	// イベントエリア設定制御
	EventAreaEnable("evb2_M05S450", 0)		//第五部  [M05S450][墓所の広い空間に出る]
	EventAreaEnable("evb2_M06S090", 0)	//第六部  [M06S090][アドルたちがセレンの園に到着する]
	EventAreaEnable("evb2_M06S210", 0)		//第六部  [M06S210][想剣ミストルティンを入手する]

	if (FLAG[SF_EVENT_ON]){

		// イベント発生制御
		if (!FLAG[GF_TBOX_DUMMY105]		// ▽第六部 [M06S210][想剣ミストルティンを入手する]
		&& FLAG[GF_06MP6308_TALK_SARAI]) {			// ▼第六部 [M06S203][サライが想念を取り戻す]
			EventAreaEnable("evb2_M06S210", 1)
		}
		else if (!FLAG[GF_06MP6350_TALK_GUARDIAN]	// ▽第六部 [M06S100][護り人たちが転移してくる]
		&& FLAG[GF_06MP6350_IN_GARDEN]) {			// ▼第六部 [M06S090][アドルたちがセレンの園に到着する]
			//SetChrPos("EV_LP_TREE", 933.99f, 0.02f, -95.82f)
		}
		else if (!FLAG[GF_06MP6350_IN_GARDEN]		// ▽第六部 [M06S090][アドルたちがセレンの園に到着する]
		&& FLAG[GF_06MP4109_GOTO_GARDEN]) { 		// ▼第六部 [M06S080][アドルがセレンの園を目指す]
			//EventAreaEnable("evb2_M06S090", 1)
			//EventCue("mp6350:EV_M06S090")
		}
		else if (!FLAG[GF_05MP6350_INTO_GARDEN]		// ▽第五部 [M05S450][墓所の広い空間に出る]
		&& FLAG[GF_TBOX_DUMMY080]) { 			// ▼第五部 [M05S440][王家の谷大ボス戦闘終了処理]
			//EventAreaEnable("evb2_M05S450", 1)
		}

	}

	// ノード設定
	
	// 初期化 ---------------------------------------------------------------------------
	
	MapAnime("himorogi" , "off" )	// ヒモロギ

	// 水道橋
	MapAnime("waterfall02" , "off" )	// 海
	MapAnime("waterfall03" , "off" )	// 氷
	MapAnime("waterfall04" , "off" )	// 蟲
	MapAnime("waterfall05" , "off" )	// 天
	// 樹（初期）
	MapHide("wood01", 0)
	MapHide("wood01_reaf", 0)
	MapAnime("wood01" , "off" )
	MapAnime("wood01_reaf" , "off" )
	MapAnime("CA_wood01" , "off" )
	MapAnime("CY_wood01" , "off" )
	// 樹（ヒドゥラ撃破後）
	MapHide("wood02", 0)
	MapHide("wood02_reaf", 0)
	MapAnime("wood02" , "off" )
	MapAnime("wood02_reaf" , "off" )
	MapAnime("CA_wood02" , "off" )
	MapAnime("CY_wood02" , "off" )
	// 樹（ミノス撃破後）
	MapHide("wood03", 0)
	MapHide("wood03_reaf", 0)
	MapAnime("wood03" , "off" )
	MapAnime("wood03_reaf" , "off" )
	MapAnime("CA_wood03" , "off" )
	MapAnime("CY_wood03" , "off" )
	// 樹（ネストール撃破後）
	MapHide("wood04", 0)
	MapHide("wood04_reaf", 0)
	MapAnime("wood04" , "off" )
	MapAnime("wood04_reaf" , "off" )
	MapAnime("CA_wood04" , "off" )
	MapAnime("CY_wood04" , "off" )
	// 樹（ウーラ撃破後）
	MapHide("wood05", 0)
	MapHide("wood05_reaf", 0)
	MapAnime("wood05" , "off" )
	MapAnime("wood05_reaf" ,"off" )
	MapAnime("CA_wood05" , "off" )
	MapAnime("CY_wood05" ,"off" )
	
	//樹完全体のオーラを消す
	MapAnime("wood05_add" , "off")

	// 水道橋 ---------------------------------------------------------------------------
	
	if (FLAG[GF_06MP6305_TALK_HYDRA]) {		// ▼第六部 [M06S143][ヒドゥラと話す]
		MapAnime("waterfall02" , "on")
		SetEnvSEPlayState(6350521,1)	//gimse waterfall02 on
		SetEnvSEPlayState(6350522,1)	//gimse waterfall02 on
	}
	if (FLAG[GF_06MP6306_TALK_MINOS]) {		// ▼第六部 [M06S163][ミノスと話す]
		MapAnime("waterfall03" , "on")
		SetEnvSEPlayState(6350531,1) 	//gimse waterfall03 on
		SetEnvSEPlayState(6350532,1) 	//gimse waterfall03 on
	}
	if (FLAG[GF_06MP6307_TALK_NESTOR]) {	// ▼第六部 [M06S183][ネストールと話す]
		MapAnime("waterfall04" , "on")
		SetEnvSEPlayState(6350541,1)	//gimse waterfall04 on
		SetEnvSEPlayState(6350542,1)	//gimse waterfall04 on
	}
	if (FLAG[GF_06MP6308_TALK_SARAI]) {		// ▼第六部 [M06S203][サライが想念を取り戻す]
		MapAnime("waterfall05" , "on")
		SetEnvSEPlayState(6350551,1)	//gimse waterfall05 on
		SetEnvSEPlayState(6350552,1)	//gimse waterfall05 on
	}
	
	// ミストルティンの樹 -------------------------------------------------------------
	
	if (FLAG[GF_06MP6308_TALK_SARAI]) {		// ▼第六部 [M06S203][サライが想念を取り戻す]
		MapHide("wood05", 1)
		MapHide("wood05_reaf", 1)
		MapAnime("wood05" , "on"  )
		MapAnime("wood05_reaf" , "on"  )
		MapAnime("CA_wood05" , "on"  )
		MapAnime("CY_wood05" , "on"  )
		MapAnime("wood05_add" ,"off" )
	}
	else if (!FLAG[GF_06MP6308_TALK_SARAI]	// ▽第六部 [M06S203][サライが想念を取り戻す]
	&& FLAG[GF_06MP6307_TALK_NESTOR]) {		// ▼第六部 [M06S183][ネストールと話す]
		MapHide("wood04", 1)
		MapHide("wood04_reaf", 1)
		MapAnime("wood04" , "on"  )
		MapAnime("wood04_reaf" , "on"  )
		MapAnime("CA_wood04" , "on"  )
		MapAnime("CY_wood04" , "on"  )
	}
	else if (!FLAG[GF_06MP6307_TALK_NESTOR]	// ▽第六部 [M06S183][ネストールと話す]
	&& FLAG[GF_06MP6306_TALK_MINOS]	) {		// ▼第六部 [M06S163][ミノスと話す]
		MapHide("wood03", 1)
		MapHide("wood03_reaf", 1)
		MapAnime("wood03" , "on"  )
		MapAnime("wood03_reaf" ,"on"  )
		MapAnime("CA_wood03" ,"on"  )
		MapAnime("CY_wood03" , "on"  )
	}
	else if (!FLAG[GF_06MP6306_TALK_MINOS]	// ▽第六部 [M06S163][ミノスと話す]
	&& FLAG[GF_06MP6305_TALK_HYDRA]) {		// ▼第六部 [M06S143][ヒドゥラと話す]
		MapHide("wood02", 1)
		MapHide("wood02_reaf", 1)
		MapAnime("wood02" , "on"  )
		MapAnime("wood02_reaf" ,"on"  )
		MapAnime("CA_wood02" , "on"  )
		MapAnime("CY_wood02" ,"on"  )
	}
	else if (!FLAG[GF_06MP6305_TALK_HYDRA]	// ▽第六部 [M06S143][ヒドゥラと話す]
	&& FLAG[GF_06MP4109_GOTO_GARDEN]) {		// ▼第六部 [M06S080][アドルがセレンの園を目指す]
		MapHide("wood01", 1)
		MapHide("wood01_reaf", 1)
		MapAnime("wood01" , "on" )
		MapAnime("wood01_reaf" , "on"  )
		MapAnime("CA_wood01" , "on" )
		MapAnime("CY_wood01" , "on"  )
	}

	// --------------------------------------------------------------------------
	
	
}


