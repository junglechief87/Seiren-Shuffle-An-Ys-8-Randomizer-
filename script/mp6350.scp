			//Intercept(INTERCEPT_STAGE12, 1)	// 時間切れ間近(赤)
		}

		CallFunc("mp6350:init")

		//マップマーカー
		DelMapMarker( SMI_SYMBOL ,PAGE_MP634x, MARKER_EV_M06S210, 0,0)		//メイン：想剣ミストルティンを入手する

		SetMapMarker( SMI_SYMBOL ,PAGE_MP6301, MARKER_EV_M06S230, -0.12f, -69.97f, 5.00f, -0.12f, -69.97f,MARKER_EV_M06S230,MN_D_MP6301,0)		//メイン：《選択の間》への扉を開く

		//イベント後の再配置
		SetChrPos("LEADER",904.65f,-0.17f,-96.00f)
		Turn("LEADER",90.00f,360.0f)
		ResetPartyPos()
		ResetFollowPoint()
		//Wait(1) //処理待ち用

		//カメラ位置復帰
		MoveCameraAt( 0 , 904.650f , -0.170f , -94.174f , 0 )	// 注視点
		ChangeCameraDistance( 0 , 8.000f , 0 )		// 基本距離
		ChangeCameraElevation( 0 , -0.796f , 0 )	// 基本仰角
		RotateCamera( 0 , -88.742f , 0 )				// 角度
		ChangeCameraPers( 0 , 60.000f , 0 )			// 視野角
		SetCameraZPlane( 0.100f , 600.000f )			// ZPlane
		RollCamera( 0 , 0.0f , 0 )				// ロール回転
		
		CallFunc("system:camera_reset")

		ResetStopFlag(STOPFLAG_EVENT)

		SetEnvSEPlayState(-1, 1)
		PlayBGM(BGM_6350_V, 0)
		FadeBGM(100,30)

		FadeIn(FADE_BLACK,FADE_FAST)
		//WaitFade()
	}
	// イベントギャラリー中
	else
	{
		FadeBGM(100,15)
		SoundEfx( SEFX_DEFAULT, 15 )
		SetEnvSEPlayState(-1, 0)	//環境SE全停止
		GalleryEvEnd(0)		// イベントギャラリー終了（スクリプトから強制的に抜けていろいろ処理してくれる）
		ResetStopFlag(STOPFLAG_EVENTIMPOSE)
	}

	//終了処理ここまで----------------------------------------------------
}

