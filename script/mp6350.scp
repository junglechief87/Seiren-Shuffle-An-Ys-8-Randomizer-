	// ResetCameraZPlane()	// イベント終了時に呼び出して下さい。マップ側の設定に戻します。

	// 位置調整・表情・モーション
	LookSpd("DANA" , 1  ,1 , 1)

	SetChrPos("ADOL", 900.73f, 0.16f, -96.00f)
	Turn("ADOL", -39.82f, 360.0f)
	SetChrPos("DANA", 900.44f, -0.86f, -96.00f)
	Turn("DANA", -100.23f, 360.0f)
	SetChrPos("LAXIA", 900.47f, -1.94f, -96.00f)
	Turn("LAXIA", -106.87f, 360.0f)
	SetChrPos("HUMMEL", 899.18f, -0.30f, -96.00f)
	Turn("HUMMEL", -85.89f, 360.0f)

