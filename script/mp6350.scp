	MapHide("wood02_reaf", 1)
	MapAnime("wood02" , "on" )
	MapAnime("wood02_reaf" , "on" )
	MapAnime("CA_wood02" , "on" )
	MapAnime("CY_wood02" , "on" )
	PlaySE(SE_M06S142_04, 80, 0, 0, 0, 1.0f, 0)//	樹が成長する
	PlaySE(SE_M06S142_05, 80, 0, 0, 0, 1.0f, 0)//	樹が成長する
	
	// カメラ待ち
	WaitThread(1002)

	Wait(45)

	SetEnvSEPlayState(-1, 0)	//環境SE全停止

	// フェードアウト
	FadeOut(FADE_BLACK, FADE_SLOW)
	WaitFade()

	//終了処理ここから----------------------------------------------------
	CallFunc("mp6350:EV_M06S142_ED")
}

