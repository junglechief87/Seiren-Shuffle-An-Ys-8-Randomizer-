	ExecuteCmd(1001, ChangeCameraDistance, INTERPOLATE_SPHERE , 9.200f , 100 )
	ExecuteCmd(1002, ChangeCameraElevation, INTERPOLATE_SPHERE , -14.621f, 100 )
	ExecuteCmd(1003, RotateCamera, INTERPOLATE_SPHERE , 327.705f, 100 )
	
	Wait(30)
	
