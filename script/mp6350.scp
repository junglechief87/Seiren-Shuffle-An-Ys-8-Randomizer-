	CrossFade(45)

	// 樹（初期をOFF）
	MapHide("wood01", 0)
	MapHide("wood01_reaf", 0)
	MapAnime("wood01" , "off" )
	MapAnime("wood01_reaf" , "off" )
	MapAnime("CA_wood01" , "off" )
	MapAnime("CY_wood01" , "off" )
	// 樹（2段階目をONに）
