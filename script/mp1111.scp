		ResetStopFlag(STOPFLAG_EVENTIMPOSE)
	}
	// 通常時
	else
	{
		PlayBGM(BGM_EVENT_016, 0)
		SetFlag(SF_NOCHANGEBGM, 1)

		LoadArg("map/mp1204v1/mp1204v1.arg")
		EventCue("mp1204v1:EV_M02S690_ED")

		ResetStopFlag(STOPFLAG_EVENTIMPOSE)
	}

//終了処理ここまで----------------------------------------------------
}

