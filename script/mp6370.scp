	ExecuteCmd(1001, ChangeCameraDistance, INTERPOLATE_SPHERE , 2.500f , 150 )
	ExecuteCmd(1002, ChangeCameraElevation, INTERPOLATE_SPHERE , -6.776f , 150 )
	ExecuteCmd(1003, RotateCamera, INTERPOLATE_SPHERE , 223.710f , 150 )
	
