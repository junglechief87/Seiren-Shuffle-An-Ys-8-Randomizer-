	WaitFade()

	// カメラ待ち
	WaitThread(1000)
	WaitThread(1001)
	WaitThread(1002)
	WaitThread(1003)
	
	Wait(20)

	// カット2 ----------------------------------------------------------------------------------------
	
	StopBGM(240)
	
	// クロスフェード
	CrossFade(FADE_NORMAL)
	
	// カメラ
	MoveCameraAt( 0 , 51.723f , -28.627f , 71.716f , 0 )	// 注視点
	ChangeCameraDistance( 0 , 4.200f , 0 )		// 基本距離
