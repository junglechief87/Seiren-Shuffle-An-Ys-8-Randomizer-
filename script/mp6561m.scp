	SetGlare(0, LERP_LINEAR, 0.700, 1.300, 1.000, 1.000, 0.600, 0.000, 1.000)	//イベント毎に調整

	//スキップ許可
	SetSkipScript("mp6561m:SubEV_Past_07_Emiy_ED")

	//開始処理ここまで-----------------------------------------------
	Wait(20)

	FadeIn(FADE_BLACK, FADE_FAST)
	WaitFade()

	//　　地下聖堂・フロア６の水晶石近くにエイミーがいる。
	//　　精霊たちと別れるイベントの後、初回話しかけるとイベント。
	TalkPopup("DANA",0,1,0,0,0)
	{
		"#000e#000mあ、あれ、エイミーちゃん？\p"
		"……どうしてここに？"
	}
	WaitPrompt()
	WaitCloseWindow()

	//	つくもの精霊エイミー
	TalkPopup("Eimy",0,2,0,0,0)
	{
		"ダーナ……\p"
		"ご存知の通り、私の存在は"
		"すでに地脈へと還りました。\p"
		"でも何とかダーナの"
		"役に立ちたくて……\p"
		"あなたに宿る皆の加護と同様、"
		"私の想いをここに留まらせました。"
	}
	WaitPrompt()
	WaitCloseWindow()

	//	"DANA"
	TalkPopup("DANA",0,1,0,0,0)
	{
		"#000e#000mそうだったんだ……"
	}
	WaitPrompt()
	WaitCloseWindow()

	//	つくもの精霊エイミー
	TalkPopup(Eimy,0,2,0,0,0)
	{
		"もう少しだけ頑張れるので、"
		"入用な物があれば言ってください。\p"
		"最期まであなたの役に立てれば"
		"私も嬉しいですから。"
	}
	WaitPrompt()
	WaitCloseWindow()

	ChangeAnimation( "DANA" , "ANI_EV_WAIT2_S", -1 , 1 )

	//	"DANA"
	TalkPopup("DANA",0,1,0,0,0)
	{
		"#000e#000mエイミーちゃん……\p"
		"うん、ありがとう。\p"
		"力を分けてくれた他の皆にも"
		"恥じないよう、精一杯頑張るよ。"
	}
	WaitPrompt()
	WaitCloseWindow()

	//	つくもの精霊エイミー
	TalkPopup("Eimy",0,2,0,0,0)
	{
		"ふふっ……はい！"
	}
	WaitPrompt()
	WaitCloseWindow()

	//　　イベント終了、操作許可。

	// フェードアウト
	FadeOut(FADE_BLACK, FADE_FAST)
	WaitFade()


	//終了処理ここから----------------------------------------------------
	CallFunc("mp6561m:SubEV_Past_07_Emiy_ED")
}

