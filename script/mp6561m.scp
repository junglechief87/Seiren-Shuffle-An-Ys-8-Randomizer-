function "NpcPosition"
{
	if (FLAG[GF_PAST_EPISODE_7_ENDEPISODE])		//過去Ⅶ
	{
		if(!FLAG[GF_SUBEV_PAST_07_KILL_IO])
		{
			SetChrPos("Eimy",-4.46f,280.66f,0.00f)
			Turn("Eimy",-153.06f,360.0f)
		}
	}
}

function "Talk_Eimy"
{
	if(!FLAG[GF_SUBEV_PAST_07_EIMY])		//	【PS4】【サブイベント：精霊エイミーと会う】
	{
		EventCue("mp6561m:SubEV_Past_07_Emiy")
	}
	else
	{
		SetStopFlag(STOPFLAG_TALKTURN)
		CallFunc("system:talk_begin_zoom")		//TALK_ZOOM

		//　　話しかけると以下の選択肢。
		//選択肢
		
		SetFlag(TF_MENU_SELECT,0)
		while( FLAG[TF_MENU_SELECT] >= 0 )	//選択肢ループ
		{
			MenuReset()
			MenuType(MENUTYPE_POPUP)
			MenuAdd(1,"話をする")
			MenuAdd(2,"精霊の品を求める")
			MenuAdd(-1,"やめる")
			MenuOpen( TF_MENU_SELECT , -1 , ADOLMENU_PPOSY , -2 , -2 , 10 , 1)
			WaitMenu(0)
			MenuClose(10,0)
		
			if(FLAG[TF_MENU_SELECT] == 1)
			{
				if(!FLAG[GF_1STTALK_PAST_7_EIMY])		//エイミーＧＦ初回
				{
					//	------------------------------------------------------------
					//▼過去編Ⅶの時代、精霊たちのお別れイベント前
					//───────────────
					//	場所		：地下聖堂・フロア１（過去）
					//───────────────
					//	配置・カメラ：入り口フロア
					//	モーション	：
					//───────────────
					//　　★ＧＦ初回
					//　　ＰＡエモーション。
					//	つくもの精霊エイミー
					TalkPopup("Eimy",0,0,0,0,0)
					{
						"ダーナ……\p"
						"えっと、その……"
					}
					WaitPrompt()
					WaitCloseWindow()

					//	"DANA"
					TalkPopup("LEADER",0,0,0,0,0)
					{
						"#000e#000mエイミーちゃん、どうかしたの？"
					}
					WaitPrompt()
					WaitCloseWindow()

					//	つくもの精霊エイミー
					TalkPopup("Eimy",0,0,0,0,0)
					{
						"……いえ、なんでもないです。\p"
						"その、今日はいい品入ってますから。"
						"たっぷり買い物していってくださいね。"
					}
					WaitPrompt()
					WaitCloseWindow()

					//	"DANA"
					TalkPopup("LEADER",0,0,0,0,0)
					{
						"#000e#000mうん、ありがとう。\p"
						"（……そうだね、準備だけは"
						"  きちんとしておこう。）"
					}
					WaitPrompt()
					WaitCloseWindow()

					SetFlag(GF_1STTALK_PAST_7_EIMY,1)
				}
				else
				{
					//　　★ＧＦ以降
					//	つくもの精霊エイミー
					TalkPopup("Eimy",0,0,0,0,0)
					{
						"その、今日はいい品入ってますから。\p"
						"どうかあなたも、たっぷり"
						"買い物していってくださいね？"
					}
					WaitPrompt()
					WaitCloseWindow()
				}
			}
			else if(FLAG[TF_MENU_SELECT] == 2)
			{
				ShopOpen(SHOP_ID_PAST1,-1)
			}
		}
		ResetStopFlag(STOPFLAG_TALKTURN)
	}
}


