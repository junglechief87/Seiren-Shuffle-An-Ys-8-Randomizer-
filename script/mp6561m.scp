	//スキップ許可
	SetSkipScript("mp6561m:SubEV_Past_OpenB6b_ED")

	//開始処理ここまで-----------------------------------------------

	PlaySE("SE_GIM_LIFT_SCFI", 100, 1,  0, 0, 1.0f, 0)	// gimse 動く音 loop

