	EventAreaEnable( "evb2_act_1602" , 0 )	//アクティブボイス

	// イベント発生制御
	if (FLAG[SF_BOSS_BATTLE])
	{
		SetWork(WK_BGM, BGM_Nothing)
		EventCue("mp6559m:EV_RetryBoss")
	}
