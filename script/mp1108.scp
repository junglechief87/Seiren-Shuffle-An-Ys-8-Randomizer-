function "act_0215"
{
	if (!FLAG[GF_AVOICE_0215])
	{
		EventAreaEnable( "evb2_act_0215" , 0 )
		EventAreaEnable( "evb2_act_0215b" , 0 )
		ActiveVoiceStop(ACTIVEVOICESTOP_WINDOWOFF)
		ActiveVoiceStart(EACT_EVID_0215, 1, 0)	//ヒュンメル合流後、少しだけ歩くと
	}
}

