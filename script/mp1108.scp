// クノス01破壊⇒壁撤去＆マーカー解除
function "EV_DELKNOS_01"
{
	MapDelProp("wall1", MNP_GROUND)
	DelMapMarker(SMI_HIVE, PAGE_F003, MARKER_HIVE_1108_01, 727, -390)
}

// クノス02破壊⇒壁撤去＆マーカー解除
