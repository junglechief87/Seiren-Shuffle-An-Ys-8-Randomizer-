	ExecuteCmd(1001, ChangeCameraDistance, INTERPOLATE_SPHERE , 20.898f , 120 )		// 基本距離
	ExecuteCmd(1002, ChangeCameraElevation, INTERPOLATE_SPHERE ,  -23.663f , 120)		// 基本仰角
