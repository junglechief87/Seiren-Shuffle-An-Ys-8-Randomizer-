	}
	// 装備設定
	EquipArmor(DANA, ICON3D_AM_023)				// ダーナ初期防具

	//HP装備付け替え分を回復
	HP_Recover( 9999 )

		// 冒険メモ処理
	SetFlag( GF_BACKUP_SF_MISSIONNO, (FLAG[SF_MISSIONNO]) )		//バックアップ
	//SetFlag( SF_MISSIONNO, MS_08_D1A )	//★冒険メモ：寺院を一回りしてから侍女アトラに話し掛けよう

	SetFlag(SF_SETRAMSAVE,1)		//リトライセーブ

}

