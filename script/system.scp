		EquipWeapon(DANA, ICON3D_WP_DANA_000)		// エーデルスフィア
	}

	//HP装備付け替え分を回復
	HP_Recover( 9999 )

		// 冒険メモ処理
	SetFlag( GF_BACKUP_SF_MISSIONNO, (FLAG[SF_MISSIONNO]) )		//バックアップ
