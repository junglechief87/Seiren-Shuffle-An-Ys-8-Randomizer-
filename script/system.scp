	GalleryEvStart(0)

	SetFlag(SF_2NDSTART,1)		//２周目判定フラグ：チュートリアルを飛ばす

	//ダーナ編①②
	if(( FLAG[TF_MENU_SELECT] >= EGALLERY_EV_03_01 && FLAG[TF_MENU_SELECT] <= EGALLERY_EV_03_03 )
	|| ( FLAG[TF_MENU_SELECT] == EGALLERY_EV_04_01 )
