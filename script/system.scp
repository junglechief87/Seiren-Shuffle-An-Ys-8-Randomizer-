		else
		{
			LoadArg("map/mp6408/mp6408.arg")
			EventCue("mp6408:SubEV_Past_06_Return_RelocateB")
		}
	}
	else if (FLAG[GF_PAST_EPISODE_7_ENDEPISODE]) {
		CallFunc("system:ReturnFrom_EPISODE_7")
		LoadArg("map/mp1201/mp1201.arg")
