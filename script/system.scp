function "SubEV_Sien_Common1"
{
	//──────────────────────────────
	//▼支援イベント共通処理①
	//──────────────────────────────

	if(!FLAG[TF_SIENEVENT])		//通常版の支援イベント
	{
		FadeBGM(70, 30)

		//　　画面暗転して以下のメッセージ。
		FadeOut(FADE_BLACK,FADE_FAST)
		WaitFade()

		//	ナレーション
		TalkPopup(UNDEF,0,3,SYSTEM_PPOSX,SYSTEM_PPOSY,0)
		{
			"#7Cリトル・パロに頼んで支援要請を出し、"
			"#7C漂流村のみんなに集まってもらった。"
		}
		WaitPrompt()
		WaitCloseWindow()

		SetFlag( TF_MENU_SELECT, (rand() % 30) )
		
		//──────────────────────
		//	⇒ランダムＡ
		if( FLAG[TF_MENU_SELECT] < 10 )
		{
			//	ドギ（ボイス再生／表示なし）
